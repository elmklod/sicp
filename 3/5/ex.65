(define (ln2-summands n)
  (cons-stream (/ 1. n)
               (stream-map - (ln2-summands (+ 1 n)))))

(define ln2-stream
  (partial-sums (ln2-summands 1)))

(define ln2
  (make-tableau euler-transform
                ln2-stream))

